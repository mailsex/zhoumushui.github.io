        <script>
            var calculateLoan = function (table, rowId) { // 合计，表格对象，对哪一列进行合计，第一列从0开始
                var trs = table.getElementsByTagName('tr');
                var start = 1, // 开始计算的行号，忽略第一行的表头
                    end = trs.length; // 结束计算的行号，忽略最后合计的一行
                var total = 0;
                for (var i = start; i < end; i++) {
                    var tdSelect = trs[i].getElementsByTagName('td')[rowId];
                    var number = parseFloat(tdSelect.innerHTML);
                    if (number)
                        total += number; // Total Interest
                }
                return total.toFixed(2); // Total
            };
            // 001-024
            document.getElementById("interest_001_024").innerHTML = calculateLoan(document.getElementById('table_loan_detail_001_024'), 2); // 支付利息
            document.getElementById("capital_001_024").innerHTML = calculateLoan(document.getElementById('table_loan_detail_001_024'), 3); // 偿还本金
            // 025-048
            document.getElementById("interest_025_048").innerHTML = calculateLoan(document.getElementById('table_loan_detail_025_048'), 2); // 支付利息
            document.getElementById("capital_025_048").innerHTML = calculateLoan(document.getElementById('table_loan_detail_025_048'), 3); // 偿还本金
            // 049-072
